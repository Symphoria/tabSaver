{
    "manifest_version": 2,

    "name": "Tab Saver",
    "description": "Save all your currently opened tabs and access them from any device any time.",
    "version": "1.0",

    "permissions": [
        "tabs",
        "storage",
        "identity"
    ],

    "browser_action": {
        "default_icon": "last-small.png",
        "default_popup": "popup.html"
    }
}